    "node-fetch": "^1.7.3",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "shx": "^0.3.4",
    "swr": "^1.1.2",
    "translation.js": "^0.7.5",
    "typescript": "^3.5.3"
  },
  "files": [
    "pontCore",
